@section Scripts {
<script type="text/javascript">
    $(document).ready(function () {
        showIfSelected('#acquisitionType', ['#solicitationSubDiv'], { valueList: ['1']});
        showIfSelected('#acquisitionType', ['#statepriceSubDiv'], {valueList: ['2']});
        showIfSelected('#printerType', ['#otherPrinterTypeDiv'], {valueList: ['1']});
        showIfSelected('#forGroupOfIndividuals', ['#uploadFileDiv'], {valueList: ['1']});
        showIfSelected('#printerReplacementCycle', ['#otherReplacementCycleDiv'], { valueList: ['0']});
        showIfSelected('#statepriceSubType',['#leaseTermsDiv'],{ valueList: ['1']});

        // $('.usd_input').mask('0000000.00', { reverse: true }); in case
        getPrinterData($('#printerType option:checked').val())
    });

    $("#acquisitionType").on("change", function () {
        showIfSelected('#acquisitionType', ['#solicitationSubDiv'], {valueList: ['1']});
        showIfSelected('#acquisitionType', ['#statepriceSubDiv'], {valueList: ['2']});
    });

    $("#printerType").on("change", function () {
        showIfSelected('#printerType', ['#otherPrinterTypeDiv'], {valueList: ['1']});
    });

     $("#statepriceSubType").on("change", function () {
        showIfSelected('#statepriceSubType',['#leaseTermsDiv'],{ valueList: ['1']});
    });


    $("#forGroupOfIndividuals").on("change", function () {
        showIfSelected('#forGroupOfIndividuals', ['#uploadFileDiv'], {valueList: ['1']});
    });

    $("#printerReplacementCycle").on("change", function () {
        showIfSelected('#printerReplacementCycle', ['#otherReplacementCycleDiv'], {valueList: ['0']});
    });


    function calculateInkCostPerPage(inkCostId, pageYieldId, costPerPageId) {
        $(costPerPageId).val(calculatePerItemCost(
            $(inkCostId).val(), $(pageYieldId).val(), 3));
    }


    function getPrinterData(printerId) {
        writeToConsole('getPrinterData [' + printerId + ']');
        var token = $('input[name="__RequestVerificationToken"]', $('form')).val();
        var myData = {
            'printerId': printerId,
            '__RequestVerificationToken': token
        };
        getJSONData("/CreateWaiverEquipmentPrinter?handler=printer", myData,
            function (data) {
                if (data.length <= 0) {
                    return;
                }
                console.log(data[0]);
                $('#purchaseAmount').val(roundFloat(data[0].equipmentPurchaseCost));
                $('#monthlyColorVolume').val(roundFloat(data[0].monthlyColorVolume));
                $('#monthlyColorVolume').prop('readonly', data[0].colorTonerDisabled);
                if (data[0].colorTonerDisabled) {
                    console.log('trying to collapse');
                    $('#colorTonerBody').collapse('hide');
                }
                $('#blackTonerCost').val(data[0].blackTonerCost);
                $('#pageYieldForBlackToner').val(data[0].pageYieldForBlackToner);
                calculateInkCostPerPage('#blackTonerCost', '#pageYieldForBlackToner', '#blackInkCostPerPage');

                $('#cyanTonerCost').val(roundFloat(data[0].cyanTonerCost));
                $('#pageYieldForCyanToner').val(data[0].pageYieldForCyanToner);
                $('#cyanTonerCost').prop('readonly', data[0].colorTonerDisabled);
                $('#pageYieldForCyanToner').prop('readonly', data[0].colorTonerDisabled);
                calculateInkCostPerPage('#cyanTonerCost', '#pageYieldForCyanToner', '#cyanInkCostPerPage');

                $('#MagentaTonerCost').val(roundFloat(data[0].MagentaTonerCost));
                $('#pageYieldForMagentaToner').val(data[0].pageYieldForMagentaToner);
                $('#MagentaTonerCost').prop('readonly', data[0].colorTonerDisabled);
                $('#pageYieldForMagentaToner').prop('readonly', data[0].colorTonerDisabled);
                calculateInkCostPerPage('#MagentaTonerCost', '#pageYieldForMagentaToner', '#magentaInkCostPerPage');

                $('#YellowTonerCost').val(roundFloat(data[0].YellowTonerCost));
                $('#pageYieldForYellowToner').val(data[0].pageYieldForYellowToner);
                $('#YellowTonerCost').prop('readonly', data[0].colorTonerDisabled);
                $('#pageYieldForYellowToner').prop('readonly', data[0].colorTonerDisabled);

                calculateInkCostPerPage('#YellowTonerCost', '#pageYieldForYellowToner', '#yellowInkCostPerPage');
            },
            function () {
                console.log('errored');
            });
    }

    $("#blackTonerCost, #pageYieldForBlackToner").on("change paste keyup", function () {
        calculateInkCostPerPage('#blackTonerCost', '#pageYieldForBlackToner', '#blackInkCostPerPage');
    });

    $("#cyanTonerCost, #pageYieldForCyanToner").on("change paste keyup", function () {
        calculateInkCostPerPage('#cyanTonerCost', '#pageYieldForCyanToner', '#cyanInkCostPerPage');
    });

    $("#MagentaTonerCost, #pageYieldForMagentaToner").on("change paste keyup", function () {
        calculateInkCostPerPage('#MagentaTonerCost', '#pageYieldForMagentaToner', '#magentaInkCostPerPage');
    });

    $("#YellowTonerCost, #pageYieldForYellowToner").on("change paste keyup", function () {
        calculateInkCostPerPage('#YellowTonerCost', '#pageYieldForYellowToner', '#yellowInkCostPerPage');
    });

    $("#printerType").on("change", function () {
        getPrinterData($('#printerType option:checked').val());
    });

    $("#purchaseAmount, #agreementOrWarrantyCost, #printerReplacementCycle, #otherReplacementCycle").on(
        "change paste keyup",
        function () {
            calculateMonthlyCostWithoutToner();
        });

    function calculateMonthlyCostWithoutToner() {
        let purchaseAmount = $('#purchaseAmount').val();
        let agreementOrWarrantyCost = $('#agreementOrWarrantyCost').val()
        writeToConsole('purchaseAmount=[' + purchaseAmount + '] agreementOrWarrantyCost=[' + agreementOrWarrantyCost + ']') ;
        if ($.isNumeric(purchaseAmount) && $.isNumeric(agreementOrWarrantyCost)) {
            let replacementMonths = $('#printerReplacementCycle option:selected').val();
            if (replacementMonths === '0') {
                replacementMonths = $('#otherReplacementCycle').val();
            }
            if ($.isNumeric(replacementMonths)) {
                let monthlyCostWithoutToner = parseFloat(purchaseAmount) + (agreementOrWarrantyCost / replacementMonths);
                $('#monthlyCostWithoutToner').val(Number(monthlyCostWithoutToner).toFixed(2));
            }
            return;
        }
        $('#monthlyCostWithoutToner').val('');
        return;
    }
</script>
}
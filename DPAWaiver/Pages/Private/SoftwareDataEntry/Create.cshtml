@page 
@model DPAWaiver.Pages.Private.SoftwareDataEntry.CreateModel  
@{ ViewData["Title"] = "Create Waiver - Software - Data Entry";}

<h2>Create</h2>
<form method="post">
    <div asp-validation-summary="All" class="text-danger"></div>

    <partial name="_UserInformation" model="@Model.UserWithDepartment" />
    @if (Model.SoftwareDataEntryWaiver.OtherFirstName != null && Model.SoftwareDataEntryWaiver.OtherLastName != null)
    {
    <div class="panel panel-primary marginTop20px">
        <div class="panel-heading">
            <h3 class="panel-title text-center">Waiver On Behalf Of</h3>
        </div>
        <div class="panel-body">

            <div class="col-md-6 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="SoftwareDataEntryWaiver.OtherFirstName" class="control-label fontWeightNormal"></label>
                    <input asp-for="SoftwareDataEntryWaiver.OtherFirstName" class="form-control" placeholder="@Html.DisplayNameFor(model => Model.SoftwareDataEntryWaiver.OtherFirstName)" />
                    <span asp-validation-for="SoftwareDataEntryWaiver.OtherFirstName" class="text-danger"></span>
                </div>
            </div>

            <div class="col-md-6 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="SoftwareDataEntryWaiver.OtherLastName" class="control-label fontWeightNormal"></label>
                    <input asp-for="SoftwareDataEntryWaiver.OtherLastName" class="form-control" placeholder="@Html.DisplayNameFor(model => Model.SoftwareDataEntryWaiver.OtherLastName)" />
                    <span asp-validation-for="SoftwareDataEntryWaiver.OtherLastName" class="text-danger"></span>
                </div>
            </div>

        </div>
    </div>
    }

<div class="panel panel-primary marginTop20px">
    <div class="panel-heading">
        <h3 class="panel-title text-center">Waiver Information</h3>
    </div>
    <div class="panel-body">
        <div class="row" style="margin-bottom: 14px">
            <div class="col-md-4 col-xs-4">
                Purpose: Software Related to Services
            </div>
            <div class="col-md-4 col-xs-4">
                Type: Data Entry
            </div>
            <div class="col-md-4 col-xs-4">
                Status: Started
            </div>
        </div>
        <div class="row">
            <div class="col-md-12 col-xs-12">
                <div class="form-group required">
                    <label asp-for="SoftwareDataEntryWaiver.projectName" class="control-label fontWeightNormal">Project Name:</label>
                    <input type="text" class="form-control" asp-for="SoftwareDataEntryWaiver.projectName" required="true" placeholder="Your Project Name">
                    <span asp-validation-for="SoftwareDataEntryWaiver.projectName" class="text-danger"></span>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 col-xs-12">
                <div class="form-group required">
                    <label asp-for="SoftwareDataEntryWaiver.SubmittedOn" class="control-label fontWeightNormal">Submitted Date:</label>
                    <input type="date" class="form-control" asp-for="SoftwareDataEntryWaiver.SubmittedOn" required="true" value="05/04/2018">
                    <span asp-validation-for="SoftwareDataEntryWaiver.SubmittedOn" class="text-danger"></span>
                </div>
            </div>
            <div class="col-md-6 col-xs-12">
                <div class="form-group required">
                    <label asp-for="SoftwareDataEntryWaiver.CostEstimate" class="control-label fontWeightNormal">Cost Estimate:</label>
                    
                    <input type="number" class="form-control" asp-for="SoftwareDataEntryWaiver.CostEstimate" required="true" value="5000.03">
                    <span asp-validation-for="SoftwareDataEntryWaiver.CostEstimate" class="text-danger"></span>
                </div>
            </div>
        </div>

    </div>
</div>

<div class="panel panel-primary marginTop20px">
    <div class="panel-heading">
        <h3 class="panel-title text-center">Waiver Details</h3>
    </div>
    <div class="panel-body">
        <div class='row'>
            <div class='col-sm-6 col-xs-12'>
                <div class='form-group required'>
                    <label asp-for='SoftwareDataEntryWaiver.typeOfSoftware' class='control-label fontWeightNormal'>Type of software being requested</label>
                    <input type='text' class='form-control' asp-for='SoftwareDataEntryWaiver.typeOfSoftware' aria-describedby='typeOfSoftware' placeholder='Type of software being requested'>
                    <span asp-validation-for="SoftwareDataEntryWaiver.typeOfSoftware" class="text-danger"></span>
                </div>
            </div>
            <div class='col-sm-6 col-xs-12'>
                <div class='form-group required'>
                    <label asp-for='SoftwareDataEntryWaiver.numberOfLicenses' class='control-label fontWeightNormal'>Number of software licenses being requested</label>
                    <input type='number' class='form-control' asp-for='SoftwareDataEntryWaiver.numberOfLicenses' aria-describedby='numberOfLicenses' placeholder='Number of software licenses being requested'>
                    <span asp-validation-for="SoftwareDataEntryWaiver.numberOfLicenses" class="text-danger"></span>
                </div>
            </div>
        </div>
        <!-- end of row -->

        <div class='row'>
            <div class='col-sm-6 col-xs-12'>
                <div class="form-group required">
                    <label asp-for="SoftwareDataEntryWaiver.newOrReplace" class="control-label fontWeightNormal">Is this new addition or replacement?</label>
                    <select class="form-control" asp-for="SoftwareDataEntryWaiver.newOrReplace">
                        <option>Additional</option>
                        <option>New</option>
                        <option>Replacement</option>
                    </select>
                    <span asp-validation-for="SoftwareDataEntryWaiver.newOrReplace" class="text-danger"></span>
                </div>
            </div>
            <div class='col-sm-6 col-xs-12'>
                <div class='form-group required'>
                    <label asp-for='SoftwareDataEntryWaiver.softwareVersion' class='control-label fontWeightNormal'>Describe software version and acquisition date</label>
                    <input type='text' class='form-control' asp-for='SoftwareDataEntryWaiver.softwareVersion' aria-describedby='softwareVersion' placeholder='Describe software version and acquisition date'>
                    <span asp-validation-for="SoftwareDataEntryWaiver.softwareVersion" class="text-danger"></span>
                </div>
            </div>
        </div>

        <div class='row'>
            <div class='col-sm-6 col-xs-12'>
                <div class='form-group required'>
                    <label asp-for='SoftwareDataEntryWaiver.costOfPresentService' class='control-label fontWeightNormal'>Cost of Present Service
                        <span class='glyphicon glyphicon-question-sign' data-toggle='tooltip' title='Cost of Present Service'></span>
                    </label>
                    <input type='number' class='form-control' asp-for='SoftwareDataEntryWaiver.costOfPresentService' aria-describedby='costOfPresentService' placeholder='Cost of Present Service'>
                    <span asp-validation-for="SoftwareDataEntryWaiver.costOfPresentService" class="text-danger"></span>
                </div>
            </div>
            <div class='col-sm-6 col-xs-12'>
                <div class='form-group required'>
                    <label asp-for='SoftwareDataEntryWaiver.currentMonthlyVolume' class='control-label fontWeightNormal'>Current Average Montly Volume</label>
                    <input type='number' class='form-control' asp-for='SoftwareDataEntryWaiver.currentMonthlyVolume' aria-describedby='currentMonthlyVolume' placeholder='Current Average Montly Volume'>
                    <span asp-validation-for="SoftwareDataEntryWaiver.currentMonthlyVolume" class="text-danger"></span>
                </div>
            </div>
        </div>
        <!-- end of row -->

        <div class="panel panel-primary marginTop20px">
            <div class="panel-heading">
                <h3 class="panel-title text-center">Average Monthly Volume For Next Five Years</h3>
            </div>
            <div class="panel-body">
                <div class="form-group required col-sm-2 col-sm-offset-1">
                    <label asp-for="SoftwareDataEntryWaiver.firstYearVolume" class="control-label fontWeightNormal">1
                        <sup>st</sup> Yr&nbsp;
                        <span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="Enter the anticipated volume for first year
This is a numeric field."></span>
                    </label>
                    <input type="number" class="form-control" asp-for="SoftwareDataEntryWaiver.firstYearVolume">
                    <span asp-validation-for="SoftwareDataEntryWaiver.firstYearVolume" class="text-danger"></span>
                </div>
                <div class="form-group required col-sm-2">
                    <label asp-for="SoftwareDataEntryWaiver.secondYearVolume" class="control-label fontWeightNormal">2
                        <sup>nd</sup> Yr&nbsp;
                        <span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="Enter the anticipated volume for second year
This is a numeric field."></span>
                    </label>
                    <input type="number" class="form-control" asp-for="SoftwareDataEntryWaiver.secondYearVolume">
                    <span asp-validation-for="SoftwareDataEntryWaiver.secondYearVolume" class="text-danger"></span>
                </div>
                <div class="form-group required col-sm-2">
                    <label asp-for="SoftwareDataEntryWaiver.thirdYearVolume" class="control-label fontWeightNormal">3
                        <sup>rd</sup> Yr&nbsp;
                        <span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="Enter the anticipated volume for third year
This is a numeric field."></span>
                    </label>
                    <input type="number" class="form-control" asp-for="SoftwareDataEntryWaiver.thirdYearVolume">
                    <span asp-validation-for="SoftwareDataEntryWaiver.thirdYearVolume" class="text-danger"></span>
                </div>
                <div class="form-group required col-sm-2">
                    <label asp-for="SoftwareDataEntryWaiver.fourthYearVolume" class="control-label fontWeightNormal">4
                        <sup>th</sup> Yr&nbsp;
                        <span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="Enter the anticipated volume for fourth year
This is a numeric field."></span>
                    </label>
                    <input type="number" class="form-control" asp-for="SoftwareDataEntryWaiver.fourthYearVolume">
                    <span asp-validation-for="SoftwareDataEntryWaiver.fourthYearVolume" class="text-danger"></span>
                </div>
                <div class="form-group required col-sm-2">
                    <label asp-for="SoftwareDataEntryWaiver.fifthYearVolume" class="control-label fontWeightNormal">5
                        <sup>th</sup> Yr&nbsp;
                        <span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="Enter the anticipated volume for fifth year
This is a numeric field."></span>
                    </label>
                    <input type="number" class="form-control" asp-for="SoftwareDataEntryWaiver.fifthYearVolume">
                    <span asp-validation-for="SoftwareDataEntryWaiver.fifthYearVolume" class="text-danger"></span>
                </div>
            </div>
        </div>

        <div class="panel panel-primary marginTop40px">
            <div class="panel-heading">
                <h3 class="panel-title text-center">If you are requesting specific equipment, please complete the following</h3>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-6 col-xs-12">
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.softwareProvider' class='control-label fontWeightNormal'>Software Provider</label>
                            <input type='text' class='form-control' asp-for='SoftwareDataEntryWaiver.softwareProvider' aria-describedby='softwareProvider' placeholder='Software Provider'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.softwareProvider" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-md-6 col-xs-12">
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.softwareVolume' class='control-label fontWeightNormal'>Software Version</label>
                            <input type='text' class='form-control' asp-for='SoftwareDataEntryWaiver.softwareVolume' aria-describedby='softwareVolume' placeholder='Software Version'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.softwareVolume" class="text-danger"></span>
                        </div>
                    </div>
                </div>

                <div class="form-group required">
                    <label asp-for="SoftwareDataEntryWaiver.selectionReason" class="control-label fontWeightNormal">Reasons for selecting this particular software:</label>
                    <textarea class="form-control" asp-for="SoftwareDataEntryWaiver.selectionReason" aria-describedby="selectionReason" placeholder="Reasons for selection"
                        rows='2'></textarea>
                    <span asp-validation-for="SoftwareDataEntryWaiver.selectionReason" class="text-danger"></span>
                </div>

                <div class="row">
                    <div class="col-md-6 col-xs-12">
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.expectedDuration' class='control-label fontWeightNormal'>Expected duration of use of software</label>
                            <input type='text' class='form-control' asp-for='SoftwareDataEntryWaiver.expectedDuration' aria-describedby='expectedDuration' placeholder='Expected duration of use of software'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.expectedDuration" class="text-danger"></span>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-6 col-xs-12">
                        <div class="form-group required">
                            <label asp-for="SoftwareDataEntryWaiver.acquisitionType" class="control-label fontWeightNormal">How do you plan to acquire this equipment?</label>
                            <select class="form-control" asp-for="SoftwareDataEntryWaiver.acquisitionType">
                                <option value="1">Solicitation</option>
                                <option value="2">State Price Agreement</option>
                                <option>Lease</option>
                                <option>Discretionary Purchase</option>
                                <option>Cancelable Rental</option>
                            </select>
                            <span asp-validation-for="SoftwareDataEntryWaiver.acquisitionType" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-md-6 col-xs-12" id="solicitationSubDiv">
                        <div class="form-group required">
                            <label asp-for="SoftwareDataEntryWaiver.solicitationSubType" class="control-label fontWeightNormal">Solicitation SubType?</label>
                            <select class="form-control" asp-for="SoftwareDataEntryWaiver.solicitationSubType">
                                <option>Discretionary</option>
                                <option>DQ</option>
                                <option>IFB</option>
                                <option>RFP</option>
                            </select>
                            <span asp-validation-for="SoftwareDataEntryWaiver.solicitationSubType" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-md-6 col-xs-12" id="statepriceSubDiv">
                        <div class="form-group required">
                            <label asp-for="SoftwareDataEntryWaiver.statepriceSubType" class="control-label fontWeightNormal">State Price SubType?</label>
                            <select class="form-control" asp-for="SoftwareDataEntryWaiver.statepriceSubType">
                                <option>Cancelable Rental</option>
                                <option>Lease</option>
                            </select>
                            <span asp-validation-for="SoftwareDataEntryWaiver.statepriceSubType" class="text-danger"></span>
                        </div>
                    </div>
                </div>



                <div class="row">
                    <div class="col-md-6 col-xs-12">
                        <div class="form-group required">
                            <label asp-for="SoftwareDataEntryWaiver.purchaseAmount" class="control-label fontWeightNormal">Purchase Amount
                                <span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="Purchase Amount"></span>
                            </label>
                            <input type="number" class="form-control" asp-for="SoftwareDataEntryWaiver.purchaseAmount" aria-describedby="purchaseAmount" placeholder="Purchase Amount">
                            <span asp-validation-for="SoftwareDataEntryWaiver.purchaseAmount" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-md-6 col-xs-12">
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.annualMaintenanceCost' class='control-label fontWeightNormal'>Annual Maintenance Renewal Cost</label>
                            <input type='number' class='form-control' asp-for='SoftwareDataEntryWaiver.annualMaintenanceCost' aria-describedby='annualMaintenanceCost' placeholder='Annual Maintenance Renewal Cost'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.annualMaintenanceCost" class="text-danger"></span>
                        </div>
                    </div>
                </div>

                <div class='row'>
                    <div class='col-sm-6 col-xs-12'>
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.operatorClassification' class='control-label fontWeightNormal'>Operator Classification</label>
                            <input type='text' class='form-control' asp-for='SoftwareDataEntryWaiver.operatorClassification' aria-describedby='operatorClassification' placeholder='Operator Classification'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.operatorClassification" class="text-danger"></span>
                        </div>
                    </div>
                    <div class='col-sm-6 col-xs-12'>
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.Grade' class='control-label fontWeightNormal'>Grade</label>
                            <input type='text' class='form-control' asp-for='SoftwareDataEntryWaiver.Grade' aria-describedby='Grade' placeholder='Grade'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.Grade" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <!-- end of row -->

                <div class='row'>
                    <div class='col-sm-6 col-xs-12'>
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.numberOfFTE' class='control-label fontWeightNormal'>Estimated Number of FTE</label>
                            <input type='number' class='form-control' asp-for='SoftwareDataEntryWaiver.numberOfFTE' aria-describedby='numberOfFTE' placeholder='Estimated Number of FTE'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.numberOfFTE" class="text-danger"></span>
                        </div>
                    </div>
                    <div class='col-sm-6 col-xs-12'>
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.hoursPerFTEPerWeek' class='control-label fontWeightNormal'>Hours Per FTE Per Week</label>
                            <input type='number' class='form-control' asp-for='SoftwareDataEntryWaiver.hoursPerFTEPerWeek' aria-describedby='hoursPerFTEPerWeek' placeholder='Hours Per FTE Per Week'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.hoursPerFTEPerWeek" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <!-- end of row -->

                <div class='row'>
                    <div class='col-sm-6 col-xs-12'>
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.totalWeeklySalary' class='control-label fontWeightNormal'>Total Weekly Salary</label>
                            <input type='number' class='form-control' asp-for='SoftwareDataEntryWaiver.totalWeeklySalary' aria-describedby='totalWeeklySalary' placeholder='Total Weekly Salary'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.totalWeeklySalary" class="text-danger"></span>
                        </div>
                    </div>
                    <div class='col-sm-6 col-xs-12'>
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.totalSpaceInSQFT' class='control-label fontWeightNormal'>Total Space Required In SQFT</label>
                            <input type='number' class='form-control' asp-for='SoftwareDataEntryWaiver.totalSpaceInSQFT' aria-describedby='totalSpaceInSQFT' placeholder='Total Space Required In SQFT'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.totalSpaceInSQFT" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <!-- end of row -->

            </div>
        </div>


        <div class="panel panel-primary marginTop20px">
            <div class="panel-heading">
                <h3 class="panel-title text-center">Overhead Cost</h3>
            </div>
            <div class="panel-body">
                <div class='row'>
                    <div class='col-sm-6 col-xs-12'>
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.monthlySupervisionAmount' class='control-label fontWeightNormal'>Monthly Supervision Amount</label>
                            <input type='number' class='form-control' asp-for='SoftwareDataEntryWaiver.monthlySupervisionAmount' aria-describedby='monthlySupervisionAmount' placeholder='Monthly Supervision Amount'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.monthlySupervisionAmount" class="text-danger"></span>
                        </div>
                    </div>
                    <div class='col-sm-6 col-xs-12'>
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.monthlyManagementAmount' class='control-label fontWeightNormal'>Monthly Management Amount</label>
                            <input type='text' class='form-control' asp-for='SoftwareDataEntryWaiver.monthlyManagementAmount' aria-describedby='monthlyManagementAmount' placeholder='Monthly Management Amount'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.monthlyManagementAmount" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <!-- end of row -->

                <div class='row'>
                    <div class='col-sm-6 col-xs-12'>
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.monthlyUtilitiesAmount' class='control-label fontWeightNormal'>Monthly Utilities Amount</label>
                            <input type='number' class='form-control' asp-for='SoftwareDataEntryWaiver.monthlyUtilitiesAmount' aria-describedby='monthlyUtilitiesAmount' placeholder='Monthly Utilities Amount'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.monthlyUtilitiesAmount" class="text-danger"></span>
                        </div>
                    </div>
                    <div class='col-sm-6 col-xs-12'>
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.monthlyIndirectCosts' class='control-label fontWeightNormal'>Monthly Indirect Costs</label>
                            <input type='number' class='form-control' asp-for='SoftwareDataEntryWaiver.monthlyIndirectCosts' aria-describedby='monthlyIndirectCosts' placeholder='Monthly Indirect Costs'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.monthlyIndirectCosts" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <!-- end of row -->

                <div class='row'>
                    <div class='col-sm-6 col-xs-12'>
                        <div class='form-group required'>
                            <label asp-for='SoftwareDataEntryWaiver.monthlyOverheadCosts' class='control-label fontWeightNormal'>Monthly Overhead Costs</label>
                            <input type='number' class='form-control' asp-for='SoftwareDataEntryWaiver.monthlyOverheadCosts' aria-describedby='monthlyOverheadCosts' placeholder='Monthly Overhead Costs'>
                            <span asp-validation-for="SoftwareDataEntryWaiver.monthlyOverheadCosts" class="text-danger"></span>
                        </div>
                    </div>
                    <div class='col-sm-6 col-xs-12'>
                    </div>
                </div>
                <!-- end of row -->


            </div>
        </div>
        <!-- end of panel -->

        <div class='form-group required'>
            <label asp-for='SoftwareDataEntryWaiver.alternativesConsidered' class='control-label fontWeightNormal'>Please describe the alternatives you examined before making this request</label>
            <textarea class='form-control' asp-for='SoftwareDataEntryWaiver.alternativesConsidered' aria-describedby='alternativesConsidered' placeholder='Please describe the alternatives you examined before making this request'
                rows='2'></textarea>
            <span asp-validation-for="SoftwareDataEntryWaiver.alternativesConsidered" class="text-danger"></span>
        </div>

        <div class="form-group">
            <label asp-for="SoftwareDataEntryWaiver.AdditionalComments" class="control-label fontWeightNormal">Any additional comments:</label>
            <textarea class="form-control" asp-for="SoftwareDataEntryWaiver.AdditionalComments" rows='2'></textarea>
            <span asp-validation-for="SoftwareDataEntryWaiver.AdditionalComments" class="text-danger"></span>
        </div>

    </div>
</div>


<div class="row add-gutter marginTop20px pull-right">
    <partial name='_Logout' />
    <a class="btn btn-default" onclick="print();">Print&nbsp;
        <span class="glyphicon glyphicon-print" aria-hidden="true"></span>
    </a>
    <button class="btn btn-default btn-space-2x" type="reset">Clear Form&nbsp;
        <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
        </button>
    <button type="submit" class="btn btn-primary" id='createwaiver' value='Submit'>
            Create Waiver&nbsp;
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
    </button>
</div>
</form>

<partial name='_Logout' />
<div style="clear: both"></div>

@section Scripts {
<environment include="Development">
    <script type="text/javascript" src="~/js/equipment.scanning.js"></script>
</environment>
<environment exclude="Development">
    <script type="text/javascript" src="~/js/equipment.scanning.min.js"></script>
</environment>
}
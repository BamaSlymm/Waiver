@page
@model DPAWaiver.Pages.Private.EquipmentPrintPress.CreateModel
@{ ViewData["Title"] = "Create Waiver - Equipment - Print/Copy- Print Press";}

<h2>Create</h2>
<form method="post">
    <div asp-validation-summary="All" class="text-danger"></div>

    <partial name="_UserInformation" Model="@Model.UserWithDepartment" />
    @if (Model.EquipmentPrintPressWaiver.OtherFirstName != null && Model.EquipmentPrintPressWaiver.OtherLastName !=
    null)
    {
    <div class="panel panel-primary marginTop20px">
        <div class="panel-heading">
            <h3 class="panel-title text-center">Waiver On Behalf Of</h3>
        </div>
        <div class="panel-body">

            <div class="col-md-6 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="EquipmentPrintPressWaiver.OtherFirstName"
                        class="control-label fontWeightNormal"></label>
                    <input asp-for="EquipmentPrintPressWaiver.OtherFirstName" class="form-control"
                        placeholder="@Html.DisplayNameFor(Model => Model.EquipmentPrintPressWaiver.OtherFirstName)" />
                    <span asp-validation-for="EquipmentPrintPressWaiver.OtherFirstName" class="text-danger"></span>
                </div>
            </div>

            <div class="col-md-6 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label asp-for="EquipmentPrintPressWaiver.OtherLastName"
                        class="control-label fontWeightNormal"></label>
                    <input asp-for="EquipmentPrintPressWaiver.OtherLastName" class="form-control"
                        placeholder="@Html.DisplayNameFor(Model => Model.EquipmentPrintPressWaiver.OtherLastName)" />
                    <span asp-validation-for="EquipmentPrintPressWaiver.OtherLastName" class="text-danger"></span>
                </div>
            </div>

        </div>
    </div>
    }

    <div class="panel panel-primary marginTop20px">
        <div class="panel-heading">
            <h3 class="panel-title text-center">Waiver Information</h3>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-md-6 col-xs-12">
                    Purpose: Equipment
                </div>
                <div class="col-md-6 col-xs-12">
                    Type: Print / Copy
                </div>
            </div>
            <div class="row" style="margin-bottom: 20px">
                <div class="col-md-6 col-xs-12">
                    Subtype: Production Press Printer
                </div>
                <div class="col-md-6 col-xs-12">
                    Status: Started
                </div>
            </div>

            <div class="row">
                <div class="col-md-12 col-xs-12">
                    <div class="form-group required">
                        <label asp-for="EquipmentPrintPressWaiver.projectName"
                            class="control-label fontWeightNormal">Project Name:</label>
                        <input type="text" class="form-control" id="EquipmentPrintPressWaiver.projectName"
                            required="true" placeholder="Your Project Name">
                        <span asp-validation-for="EquipmentPrintPressWaiver.projectName" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6 col-xs-12">
                    <div class="form-group required">
                        <label asp-for="EquipmentPrintPressWaiver.SubmittedOn"
                            class="control-label fontWeightNormal">Submitted Date:</label>
                        <input type="date" class="form-control" id="EquipmentPrintPressWaiver.SubmittedOn"
                            required="true" value="05/04/2018">
                        <span asp-validation-for="EquipmentPrintPressWaiver.SubmittedOn" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-6 col-xs-12">
                    <div class="form-group required">
                        <label asp-for="EquipmentPrintPressWaiver.CostEstimate"
                            class="control-label fontWeightNormal">Project Budget/Cost Estimate:</label>
                        <span class="glyphicon glyphicon-info-sign" data-toggle="tooltip"
                            title="Enter the project cost and estimate such as 9999.99"></span>
                        <input type="number" class="form-control" id="EquipmentPrintPressWaiver.CostEstimate"
                            required="true" step="0.03">
                        <span asp-validation-for="EquipmentPrintPressWaiver.CostEstimate" class="text-danger"></span>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="panel panel-primary marginTop20px">
        <div class="panel-heading">
            <h3 class="panel-title text-center">Waiver Details</h3>
        </div>
        <div class="panel-body">
            <div class='form-group required'>
                <label asp-for='EquipmentPrintPressWaiver.justificationDescription'
                    class='control-label fontWeightNormal'>Justification For Request</label>
                <textarea class='form-control' id='EquipmentPrintPressWaiver.justificationDescription'
                    aria-describedby='justificationDescription' placeholder='Justification For Request'
                    rows='2'></textarea>
                <span asp-validation-for="EquipmentPrintPressWaiver.justificationDescription"
                    class="text-danger"></span>
            </div>
            <div class="form-group required">
                <label asp-for="EquipmentPrintPressWaiver.servicesReceived" class="control-label fontWeightNormal">How
                    do you currently meet this need?</label>
                <textarea class="form-control" id="EquipmentPrintPressWaiver.servicesReceived" rows="2"
                    placeholder="How do you currently meet this need?"></textarea>
                <span asp-validation-for="EquipmentPrintPressWaiver.servicesReceived" class="text-danger"></span>
            </div>
            <div class='row'>
                <div class='col-sm-6 col-xs-12'>
                    <div class="form-group required">
                        <label asp-for="EquipmentPrintPressWaiver.newOrReplace"
                            class="control-label fontWeightNormal">Is this new addition or replacement?</label>
                        <select class="form-control" id="EquipmentPrintPressWaiver.newOrReplace">
                            <option value='0'>Additional</option>
                            <option>New</option>
                            <option value='1'>Replacement</option>
                        </select>
                        <span asp-validation-for="EquipmentPrintPressWaiver.newOrReplace" class="text-danger"></span>
                    </div>
                </div>
                <div class='col-sm-6 col-xs-12' id='currentMakeDiv'>
                    <div class="form-group required">
                        <label asp-for="EquipmentPrintPressWaiver.currentMake"
                            class="control-label fontWeightNormal">Current Make</label>
                        <input type="text" class="form-control" id="EquipmentPrintPressWaiver.currentMake"
                            aria-describedby="currentMake" placeholder="Current Make">
                        <span asp-validation-for="EquipmentPrintPressWaiver.currentMake" class="text-danger"></span>
                    </div>
                </div>
            </div>

            <div class="row" id='ModelAndAcquisitionDiv'>
                <div class="col-md-6 col-xs-12">
                    <div class="form-group required">
                        <label asp-for="EquipmentPrintPressWaiver.currentModel"
                            class="control-label fontWeightNormal">Current Model</label>
                        <input type="text" class="form-control" id="EquipmentPrintPressWaiver.currentModel"
                            aria-describedby="currentModel" placeholder="Current Model">
                        <span asp-validation-for="EquipmentPrintPressWaiver.currentModel" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-6 col-xs-12">
                    <div class="form-group required">
                        <label asp-for="EquipmentPrintPressWaiver.acquisitionDate"
                            class="control-label fontWeightNormal">Acquisition Date</label>
                        <input type="date" class="form-control" id="EquipmentPrintPressWaiver.acquisitionDate"
                            aria-describedby="acquisitionDate" placeholder="Acquisition Date">
                        <span asp-validation-for="EquipmentPrintPressWaiver.acquisitionDate" class="text-danger"></span>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-6 col-xs-12">
                    <div class="form-group required">
                        <label asp-for="EquipmentPrintPressWaiver.monthlyCost"
                            class="control-label fontWeightNormal">Monthly Cost of Current Service</label>
                        <input type="number" class="form-control" id="EquipmentPrintPressWaiver.monthlyCost"
                            aria-describedby="monthlyCost" placeholder="Monthly Cost of Current Service">
                        <span asp-validation-for="EquipmentPrintPressWaiver.monthlyCost" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-6 col-xs-12">
                    <div class='form-group required'>
                        <label asp-for='EquipmentPrintPressWaiver.averageMonthlyVolume'
                            class='control-label fontWeightNormal'>Current Average Monthly Volume
                            <span class='glyphicon glyphicon-question-sign' data-toggle='tooltip'
                                title='Enter the current average montly volume. This is a numeric field.'></span>
                        </label>
                        <input type='number' class='form-control' id='EquipmentPrintPressWaiver.averageMonthlyVolume'
                            aria-describedby='averageMonthlyVolume' placeholder='Current Average Monthly Volume'>
                        <span asp-validation-for="EquipmentPrintPressWaiver.averageMonthlyVolume"
                            class="text-danger"></span>
                    </div>
                </div>
            </div>

            <div class="panel panel-primary marginTop20px">
                <div class="panel-heading">
                    <h3 class="panel-title text-center">Anticipated Annual Volume for next five years</h3>
                </div>
                <div class="panel-body">
                    <div class="form-group required col-sm-2 col-sm-offset-1">
                        <label asp-for="EquipmentPrintPressWaiver.firstYearVolume"
                            class="control-label fontWeightNormal">1
                            <sup>st</sup> Yr&nbsp;
                            <span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="Enter the anticipated volume for first year
This is a numeric field."></span>
                        </label>
                        <input type="number" class="form-control" id="EquipmentPrintPressWaiver.firstYearVolume">
                        <span asp-validation-for="EquipmentPrintPressWaiver.firstYearVolume" class="text-danger"></span>
                    </div>
                    <div class="form-group required col-sm-2">
                        <label asp-for="EquipmentPrintPressWaiver.secondYearVolume"
                            class="control-label fontWeightNormal">2
                            <sup>nd</sup> Yr&nbsp;
                            <span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="Enter the anticipated volume for second year
This is a numeric field."></span>
                        </label>
                        <input type="number" class="form-control" id="EquipmentPrintPressWaiver.secondYearVolume">
                        <span asp-validation-for="EquipmentPrintPressWaiver.secondYearVolume"
                            class="text-danger"></span>
                    </div>
                    <div class="form-group required col-sm-2">
                        <label asp-for="EquipmentPrintPressWaiver.thirdYearVolume"
                            class="control-label fontWeightNormal">3
                            <sup>rd</sup> Yr&nbsp;
                            <span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="Enter the anticipated volume for third year
This is a numeric field."></span>
                        </label>
                        <input type="number" class="form-control" id="EquipmentPrintPressWaiver.thirdYearVolume">
                        <span asp-validation-for="EquipmentPrintPressWaiver.thirdYearVolume" class="text-danger"></span>
                    </div>
                    <div class="form-group required col-sm-2">
                        <label asp-for="EquipmentPrintPressWaiver.fourthYearVolume"
                            class="control-label fontWeightNormal">4
                            <sup>th</sup> Yr&nbsp;
                            <span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="Enter the anticipated volume for fourth year
This is a numeric field."></span>
                        </label>
                        <input type="number" class="form-control" id="EquipmentPrintPressWaiver.fourthYearVolume">
                        <span asp-validation-for="EquipmentPrintPressWaiver.fourthYearVolume"
                            class="text-danger"></span>
                    </div>
                    <div class="form-group required col-sm-2">
                        <label asp-for="EquipmentPrintPressWaiver.fifthYearVolume"
                            class="control-label fontWeightNormal">5
                            <sup>th</sup> Yr&nbsp;
                            <span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="Enter the anticipated volume for fifth year
This is a numeric field."></span>
                        </label>
                        <input type="number" class="form-control" id="EquipmentPrintPressWaiver.fifthYearVolume">
                        <span asp-validation-for="EquipmentPrintPressWaiver.fifthYearVolume" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="panel panel-primary marginTop20px">
                <div class="panel-heading">
                    <h3 class="panel-title text-center">The type of equipment being requested</h3>
                </div>
                <div class="panel-body">
                    <div class='row'>
                        <div class='col-sm-6 col-xs-12'>
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.equipmentType'
                                    class='control-label fontWeightNormal'>Type of Equipment</label>
                                <input type='text' class='form-control' id='EquipmentPrintPressWaiver.equipmentType'
                                    aria-describedby='equipmentType' placeholder='Type of Equipment'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.equipmentType"
                                    class="text-danger"></span>
                            </div>
                        </div>
                        <div class='col-sm-6 col-xs-12'>
                            <div class="form-group required">
                                <label asp-for="EquipmentPrintPressWaiver.NumberofEquipment"
                                    class="control-label fontWeightNormal">Number of equipment?
                                    <span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="How many pieces of equipment are being requested?
    This is a numeric field."></span>
                                </label>
                                <input type="number" class="form-control"
                                    id="EquipmentPrintPressWaiver.NumberofEquipment"
                                    aria-describedby="NumberofEquipment" placeholder="Number of Equipment, Integer">
                                <span asp-validation-for="EquipmentPrintPressWaiver.NumberofEquipment"
                                    class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                    <div class='row' id='otherequipmentTypeDiv'>
                        <div class='col-sm-6 col-xs-12'>
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.Make'
                                    class='control-label fontWeightNormal'>Make</label>
                                <input type='text' class='form-control' id='EquipmentPrintPressWaiver.Make'
                                    aria-describedby='Make' placeholder='Make'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.Make" class="text-danger"></span>
                            </div>
                        </div>
                        <div class='col-sm-6 col-xs-12'>
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.Model'
                                    class='control-label fontWeightNormal'>Model</label>
                                <input type='text' class='form-control' id='EquipmentPrintPressWaiver.Model'
                                    aria-describedby='Model' placeholder='Model'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.Model" class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                    <!-- end of row -->
                    <div class="row">
                        <div class="col-md-6 col-xs-12">
                            <div class="form-group required">
                                <label asp-for="EquipmentPrintPressWaiver.acquisitionType"
                                    class="control-label fontWeightNormal">How do you plan to acquire this
                                    equipment?</label>
                                <select class="form-control" id="EquipmentPrintPressWaiver.acquisitionType">
                                    <option value="1">Solicitation</option>
                                    <option value="2">State Price Agreement</option>
                                    <option value="3">Lease</option>
                                    <option value="4">Discretionary Purchase</option>
                                    <option>Cancelable Rental</option>
                                </select>
                                <span asp-validation-for="EquipmentPrintPressWaiver.acquisitionType"
                                    class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-6 col-xs-12" id="solicitationSubDiv">
                            <div class="form-group required">
                                <label asp-for="EquipmentPrintPressWaiver.solicitationSubType"
                                    class="control-label fontWeightNormal">Solicitation SubType?</label>
                                <select class="form-control" id="EquipmentPrintPressWaiver.solicitationSubType">
                                    <option>Discretionary</option>
                                    <option>DQ</option>
                                    <option>IFB</option>
                                    <option>RFP</option>
                                </select>
                                <span asp-validation-for="EquipmentPrintPressWaiver.solicitationSubType"
                                    class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-6 col-xs-12" id="statepriceSubDiv">
                            <div class="form-group required">
                                <label asp-for="EquipmentPrintPressWaiver.statePriceSubType"
                                    class="control-label fontWeightNormal">State Price SubType?</label>
                                <select class="form-control" id="EquipmentPrintPressWaiver.statePriceSubType">
                                    <option>Cancelable Rental</option>
                                    <option>Lease</option>
                                </select>
                                <span asp-validation-for="EquipmentPrintPressWaiver.statePriceSubType"
                                    class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-6 col-xs-12" id="acquisitionTypeDiv">
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.acquisitionType'
                                    class='control-label fontWeightNormal'>Purchase Amount</label>
                                <input type='number' class='form-control' id='EquipmentPrintPressWaiver.acquisitionType'
                                    aria-describedby='acquisitionType' placeholder='Purchase Amount'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.acquisitionType"
                                    class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-6 col-xs-12" id="leaseDurationDiv">
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.leaseDuration'
                                    class='control-label fontWeightNormal'>Lease Duration</label>
                                <select class='form-control' id='EquipmentPrintPressWaiver.leaseDuration'
                                    aria-describedby='leaseDuration'>
                                    <option value='48'>48 months</option>
                                    <option value='60'>60 months</option>
                                    <option value='0'>Other</option>
                                </select>
                                <span asp-validation-for="EquipmentPrintPressWaiver.leaseDuration"
                                    class="text-danger"></span>
                            </div>
                        </div>
                    </div>

                    <div class='row' id='leaseTermsDiv'>
                        <div class='col-sm-6 col-xs-12'>
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.numberOfMonths'
                                    class='control-label fontWeightNormal'>Number of Months of Lease</label>
                                <input type='number' class='form-control' id='EquipmentPrintPressWaiver.numberOfMonths'
                                    aria-describedby='numberOfMonths' placeholder='Number of Months of Lease'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.numberOfMonths"
                                    class="text-danger"></span>
                            </div>
                        </div>
                        <div class='col-sm-6 col-xs-12'>
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.monthlyLeaseAmount'
                                    class='control-label fontWeightNormal'>Monthly Lease Amount</label>
                                <input type='number' class='form-control'
                                    id='EquipmentPrintPressWaiver.monthlyLeaseAmount'
                                    aria-describedby='monthlyLeaseAmount' placeholder='Monthly Lease Amount'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.monthlyLeaseAmount"
                                    class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                    <!-- end of row -->
                    <div class='form-group required'>
                        <label asp-for='EquipmentPrintPressWaiver.makeAndModel'
                            class='control-label fontWeightNormal'>Make and Model</label>
                        <input type='text' class='form-control' id='EquipmentPrintPressWaiver.makeAndModel'
                            aria-describedby='makeAndModel' placeholder='Make and Model'>
                        <span asp-validation-for="EquipmentPrintPressWaiver.makeAndModel" class="text-danger"></span>
                    </div>
                    <div class="form-group required">
                        <label asp-for="EquipmentPrintPressWaiver.selectionReason"
                            class="control-label fontWeightNormal">Reasons for selecting this particular
                            equipment:</label>
                        <textarea class="form-control" id="EquipmentPrintPressWaiver.selectionReason"
                            aria-describedby="selectionReason" placeholder="Reasons for selection" rows='2'></textarea>
                        <span asp-validation-for="EquipmentPrintPressWaiver.selectionReason" class="text-danger"></span>
                    </div>
                    <div class='row'>
                        <div class='col-sm-6 col-xs-12'>
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.productionCapacity'
                                    class='control-label fontWeightNormal'>Production Capacity (throughput)
                                    <span class='glyphicon glyphicon-question-sign' data-toggle='tooltip'
                                        title='Production capacity (how many sheets per) also throughput'></span>
                                </label>
                                <input type='number' class='form-control'
                                    id='EquipmentPrintPressWaiver.productionCapacity'
                                    aria-describedby='productionCapacity'
                                    placeholder='Production Capacity (throughput)'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.productionCapacity"
                                    class="text-danger"></span>
                            </div>
                        </div>
                        <div class='col-sm-6 col-xs-12'>
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.estimatedCost'
                                    class='control-label fontWeightNormal'>Estimated Cost of Equipment</label>
                                <input type='number' class='form-control' id='EquipmentPrintPressWaiver.estimatedCost'
                                    aria-describedby='estimatedCost' placeholder='Estimated Cost of Equipment'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.estimatedCost"
                                    class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                    <div class='form-group required'>
                        <label asp-for='EquipmentPrintPressWaiver.leaseOrPurchase'
                            class='control-label fontWeightNormal'>Lease or purchase</label>
                        <select class='form-control' id='EquipmentPrintPressWaiver.leaseOrPurchase'
                            aria-describedby='leaseOrPurchase'>
                            <option value='0'>Lease</option>
                            <option value='1'>Purchase</option>
                        </select>
                        <span asp-validation-for="EquipmentPrintPressWaiver.leaseOrPurchase" class="text-danger"></span>
                    </div>
                    <div class='row' id='leaseDataDiv'>
                        <div class='col-sm-6 col-xs-12'>
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.numberOfYears'
                                    class='control-label fontWeightNormal'>Number of Years</label>
                                <input type='number' class='form-control' id='EquipmentPrintPressWaiver.numberOfYears'
                                    aria-describedby='numberOfYears' placeholder='Number of Years'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.numberOfYears"
                                    class="text-danger"></span>
                            </div>
                        </div>
                        <div class='col-sm-6 col-xs-12'>
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.costOfLeasePerYear'
                                    class='control-label fontWeightNormal'>Cost of lease per year</label>
                                <input type='number' class='form-control'
                                    id='EquipmentPrintPressWaiver.costOfLeasePerYear'
                                    aria-describedby='costOfLeasePerYear' placeholder='Cost of lease per year'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.costOfLeasePerYear"
                                    class="text-danger"></span>
                                <!-- TODO Hover help: Total lease ??? -->
                            </div>
                        </div>
                    </div>

                    <div class='row' id='purchaseDataDiv'>
                        <div class='col-sm-6 col-xs-12'>
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.expectedUsefulLife'
                                    class='control-label fontWeightNormal'>Expected useful life of this
                                    equipment</label>
                                <input type='number' class='form-control'
                                    id='EquipmentPrintPressWaiver.expectedUsefulLife'
                                    aria-describedby='expectedUsefulLife'
                                    placeholder='Expected useful life of this equipment'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.expectedUsefulLife"
                                    class="text-danger"></span>
                            </div>
                        </div>
                        <div class='col-sm-6 col-xs-12'>
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.depreciationCostPerYear'
                                    class='control-label fontWeightNormal'>Depreciation Cost Per Year</label>
                                <input type='number' class='form-control'
                                    id='EquipmentPrintPressWaiver.depreciationCostPerYear'
                                    aria-describedby='depreciationCostPerYear' placeholder='Depreciation Cost Per Year'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.depreciationCostPerYear"
                                    class="text-danger"></span>
                            </div>
                        </div>
                    </div>

                    <div class='row'>
                        <div class='col-sm-6 col-xs-12'>
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.annualMaintenanceCostPerYear'
                                    class='control-label fontWeightNormal'>Cost of Annual Maintenance Per Year</label>
                                <input type='number' class='form-control'
                                    id='EquipmentPrintPressWaiver.annualMaintenanceCostPerYear'
                                    aria-describedby='annualMaintenanceCostPerYear'
                                    placeholder='Cost of Annual Maintenance Per Year'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.annualMaintenanceCostPerYear"
                                    class="text-danger"></span>
                            </div>
                        </div>
                        <div class='col-sm-6 col-xs-12'>
                            <div class='form-group required'>
                                <label asp-for='EquipmentPrintPressWaiver.suppliesCost'
                                    class='control-label fontWeightNormal'>Cost of Supplies
                                    <span class='glyphicon glyphicon-question-sign' data-toggle='tooltip'
                                        title='Consumables not included in Maintenance'></span>
                                </label>
                                <input type='string' class='form-control' id='EquipmentPrintPressWaiver.suppliesCost'
                                    aria-describedby='suppliesCost'
                                    placeholder='Cost of Supplies - Consumables not included in the Maintenance'>
                                <span asp-validation-for="EquipmentPrintPressWaiver.suppliesCost"
                                    class="text-danger"></span>
                                <!-- TODO: Is this per year or over all. Also what does consumables not included, does that refer to cost of annual maintenance needs hover or a help -->
                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel panel-primary marginTop20px">
